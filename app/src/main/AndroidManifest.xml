<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.arintv.conlabs.witopentrack">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".examples.Activity_Example_URLScheme"></activity>
        <activity
            android:name=".examples.Activity_Example_LocationManager"
            android:screenOrientation="portrait" />
        <activity
            android:name=".examples.Activity_Example_MapLevel"
            android:screenOrientation="portrait" />
        <activity
            android:name=".App_Activity_Main"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".App_Menu_Item"
            android:screenOrientation="portrait" />
        <activity
            android:name=".examples.Activity_Example_GPSTrack"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="SET_YOUR_KAKAO_DEVELOPMENT_KEY" />
    </application>

</manifest>